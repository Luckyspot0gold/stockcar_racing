import { Button } from '@avalabs/builderkit';
 
// Basic usage
<Button 
  label="Click me"
  action={() => console.log('Button clicked')}
/>
 
// With loading state
<Button 
  label="Processing..."
  action={() => {}}
  status="loading"
/>
 
// Disabled state
<Button 
  label="Unavailable"
  action={() => {}}
  status="disabled"
/>
